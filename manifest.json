{
  "manifest_version": 3,
  "name": "CNC kedarnath",
  "version": "0.1",
  "author": "codeandcore",
  "description": "Lorem ipsum",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
